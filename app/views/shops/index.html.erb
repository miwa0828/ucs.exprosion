   <div class='container'>
    <div class='row'>
       <h2>投稿した新着情報一覧</h2>
        <% @post.each do |post|%>
          <%= post.title %>
          <%= post.detail %>
          <%= attachment_image_tag post, :image, :fill, 100, 100, format: 'jpeg' %>
          <%= link_to "編集", edit_post_path(post.id) %>
          <%= link_to '削除する', post_path(post.id), method: :delete,'data-confirm' => '本当に削除しますか？'  %>
        <% end %>



       <h2>登録した古着屋一覧</h2>
        <% @shop.each do |shop| %>
          <%= shop.name %>
          <%= shop.detail %>
          <%= shop.address %>
          <%= shop.open_time %>
          <%= shop.close_time %>
          <%= shop.phone_number %>
          <%= shop.image %>
          <%= shop.url %>
          <%= link_to "編集", edit_shop_path(current_owner.id) %>
          <%= link_to '削除する', shop_path(shop.id), method: :delete,'data-confirm' => '本当に削除しますか？'  %>
        <% end %>
    </div>
  </div>